---
title: Chateau
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title }}</title>
    <link rel="stylesheet" href="/static/css/style.css">
  </head>
  <body>
    <header>
      Header 
      <nav>
        <ul>
          {% for item in collections.all | eleventyNavigation %}
            <li><a href="{{ item.url }}">{{ item.title }}</a></li>
          {% endfor %}
          {% if not settings.blognav %}
            <li><a href="/blog/">Blog</a></li>
          {% endif %}          
        </ul>
      </nav>
    </header>
    <main>
      {{ content | safe }}
    </main>
    {% if footerlinks.links %}
      <footer>
        <ul>
          {% for item in footerlinks.links %}
            <li>
              <a href="{{ item.url }}">{{ item.title }}</a>
              <p>{{ item.desc }}</p>
            </li>
          {% endfor %}
        </ul>
      </footer>
    {% endif %}
  </body>
</html>